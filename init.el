 ;; -*- lexical-binding: t -*-;; The default is 800 kilobytes.  Measured in bytes.(setq gc-cons-threshold (* 50 1000 1000))(add-to-list 'load-path "~/.emacs.d/lisp/")(defun efs/display-startup-time ()  (message "Emacs loaded in %s with %d garbage collections."           (format "%.2f seconds"                   (float-time                   (time-subtract after-init-time before-init-time)))           gcs-done))(add-hook 'emacs-startup-hook #'efs/display-startup-time)(require 'init-basic)(require 'init-packages)(require 'init-funcs)(require 'init-evil)(require 'init-ui)(require 'init-dashboard)(require 'init-completion)(require 'init-tools)(require 'init-keybindings)(require 'init-org)(require 'init-programming)(require 'init-translation);; Make gc pauses faster by decreasing the threshold.(setq gc-cons-threshold (* 2 1000 1000))(setq custom-file (expand-file-name "~/.emacs.d/custom.el"))(load custom-file 'nxo-error 'no-message)